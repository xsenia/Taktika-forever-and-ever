    // renderContent('4400')
    console.log(' ');
  }

  setTimeout(function () {
    scrollTo('.signup');
  }, 400);
  friendRegistration();
}

$(".js-to-reg").on('click', function (e) {
  if ($('.signup').length) {
    e.preventDefault();
    scrollTo('.signup');
  } else {
    return;
  }
}); //Scroll to elem

function slideTo(elem) {
  var offset = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : -80;
  $([document.documentElement, document.body]).animate({
    scrollTop: $(elem).offset().top + offset
  }, 800);
}

$(document).click(function (e) {
  var $elem = $(e.target);

  if (!$elem.is('[rel="modal:open"]') && $elem.is('a, a *')) {
    var href = $elem.closest('a').attr('href');

    if (/^#/.test(href)) {
      e.preventDefault();
      var offset = _uaParser["default"].getDevice().type === 'mobile' ? -150 : -80;
      slideTo(href, offset);
    }
  }
});

