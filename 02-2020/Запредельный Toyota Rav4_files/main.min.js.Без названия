var getEquipmentImageUrl = function getEquipmentImageUrl(_ref) {
  var equipmentId = _ref.equipmentId,
      colorId = _ref.colorId,
      type = _ref.type;
  var size = {
    width: 1000,
    height: 370
  };

  if (type === 'preview') {
    size = {
      width: 250,
      height: 87
    };
  }

  var modelId = 'a68a58fb-a10e-41ae-9459-3a7c4060500f';
  var angle = type === 'main' ? '03' : '09';
  return "https://images.toyota-europe.com/url/renderSpecificVehicle.ashx?brand=Toyota&country=RU&model=".concat(modelId, "&car=").concat(equipmentId, "&exterior=").concat(colorId, "&upholstery=00000000-0000-0000-0000-000000000000&view=exterior&angle=").concat(angle, "&format=png&mode=DAY&image-quality=75&scale-mode=1&width=").concat(size.width, "&height=").concat(size.height, "&MoreInfo=true");
};

var getEquipments = function getEquipments() {
  return (0, _libs.makeRequest)({
    url: API,
    params: {
      query: "{\n              car_complectations(car_model_id: 2939) {\n                id\n                name\n                transmission\n                engine_name\n                wheel_drive\n                car_db_id\n                engine_power\n                exterior_colors {\n                  hex_color\n                  car_db_id\n                  id\n                  name\n                  internal_code\n                }\n              }\n            }\n        "
    }
  }).then(function (res) {
    var data = res.data.data.car_complectations;
    var colors = {};
    var equipments = {};
    var custom = {};
    var equipments_merged = {};
    var equipmentSelected = data[0].name;
    var variants = {};
    data.forEach(function (item) {
      if (!equipments_merged[item.name]) {
        equipments_merged[item.name] = [];
      }

      variants[item.id] = {
        id: item.id,
        engine: {
          name: item.engine_name,
          power: item.engine_power,
          drive: item.wheel_drive,
          transmission: item.transmission
        },
        // price: item.price
        price: equipPrices[item.id]
      };
      equipments_merged[item.name].push(item);
    });
    Object.values(equipments_merged).forEach(function (variants, index) {
      var item = variants[0];

      if (!custom[item.name]) {
        custom[item.name] = {};
      }

      custom[item.name].color = item.exterior_colors[0].id;
      custom[item.name].variant = item.id;
      var images = {};
      item.exterior_colors.forEach(function (color) {
        images[color.id] = getEquipmentImageUrl({
          equipmentId: item.car_db_id,
          colorId: color.car_db_id,
          type: 'main'
        });
      });
      equipments[item.name] = {
        preview: getEquipmentImageUrl({
          equipmentId: item.car_db_id,
          colorId: item.exterior_colors[0].car_db_id,
          type: 'preview'
        }),
        name: item.name,
        variants: variants.map(function (item) {
          return item.id;
        }),
        colors: item.exterior_colors.map(function (color) {
          return color.id;
        }),
        weight: index,
        images: images
      };
      item.exterior_colors.forEach(function (color) {
        colors[color.id] = {
          id: color.id,
          value: color.hex_color,
          name: color.name,
          // extraCharge: color.list_price
          extraCharge: colorExtraCharge[color.internal_code]
        };
      });
    });
    return {
      colors: colors,
      equipments: equipments,
      custom: custom,
      equipmentSelected: equipmentSelected,
      variants: variants
    };
  });
};

