var formEventSubmit = function formEventSubmit() {
  var formData = {
    firstname: $('input[name="customer_first_name"]').val(),
    lastname: $('input[name="customer_last_name"]').val(),
    middlename: $('input[name="customer_patronymic"]').val(),
    birthdate: $('input[name="customer_date_of_birth"]').val().slice(0, 10).split('.').reverse().join('-'),
    phone: $('input[name="customer_phone"]').val(),
    email: $('input[name="customer_email"]').val(),
    city_id: window.currentCity,
    dealer_id: Number($('select[name="dealer_center_id"]').val()),
    car_buying_planed_id: isNaN(Number($('input[name="buy_car"]:checked').val())) ? Number($('select[name="buy_car"]').val()) : Number($('[name="buy_car"]:checked').val()),
    car_brand_id: Number($('select[name="car_brand_id"]').val()),
    car_model_id: Number($('select[name="car_model_id"]').val()),
    gender: typeof $("input[name=customer_sex]:checked").val() == "undefined" ? $("select[name=customer_sex]").val() : $("input[name=customer_sex]:checked").val(),
    subscription_agree: $("input[name=subscription_agree]:checked").val() || "0",
    eventId: '1'
  };

  if ($('.isServerError').length > 0) {
    $('.isServerError')[0].scrollIntoView({
      block: "center"
    });
    return;
  }

  (0, _api.smsAuth)(formData.phone).then(function (sms) {
    (0, _libs.requestLoading)(false);
    saveData(formData, 'eventFormData');
    saveData(sms, 'smsData');
    showSmsPopup();
  });

  function showSmsPopup() {
    $('.b-popup_sms').addClass('active');
  }
};

getParameterByName('promocode') ? $('.signup-form').validate(formFriend) : $('.signup-form').validate(form); //Friend Registration

