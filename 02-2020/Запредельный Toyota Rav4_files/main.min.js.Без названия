/* global $ */
$(function () {
  (function () {
    var resize = function resize() {
      $('.accordion-tab__content-wrap').each(function (i, item) {
        $(item).css('height', $(item).find('.accordion-tab__content').outerHeight());
      });
    };

    $(window).resize(resize);
    resize();
  })();

  $(document).click(function (e) {
    var classActive = 'accordion--active';
    var classItemActive = 'accordion__item--active';
    var header = $(e.target).hasClass('accordion-tab__header') ? $(e.target) : $(e.target).parents('.accordion-tab__header');
    if (!header.length) return;
    var item = header.parents('.accordion-tab');
    var list = item.parents('.accordion');

    if (item.hasClass(classItemActive)) {
      item.removeClass(classItemActive);
      list.removeClass(classActive);
    } else {
      list.addClass(classActive);
      list.find('.accordion__item').removeClass(classItemActive);
      item.addClass(classItemActive);
    }
  });
});

