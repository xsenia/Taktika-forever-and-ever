$.validator.addMethod('checkCyrilic', function (value, element, params) {
  return !/[^--- ]/.test(value);
});
$.validator.addMethod("birthdate", function (value, element) {
  var year = value.split('.');
  if (value.match(/^\d\d?\.\d\d?\.\d\d\d\d$/) && parseInt(year[0]) <= 31 && parseInt(year[1]) <= 12 && parseInt(year[2]) > 1900) return true;else return false;
}); //Event Form

