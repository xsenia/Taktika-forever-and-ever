var placeSearch, autocomplete;
var checkPopUpForm = {
  checker_city: 'checker_city'
};

window.initAutocomplete = function () {
  autocomplete = new google.maps.places.Autocomplete(document.getElementById('checker_city'), {
    types: ['(cities)'],
    componentRestrictions: {
      country: ['ru', 'by']
    },
    language: 'ru'
  });
  autocomplete.addListener('place_changed', onPlaceChange);
};

function geolocate() {
  if (navigator.geolocation) {
    var onSuccessLocate = function onSuccessLocate(position) {
      var geolocation = {
        lat: position.lat || position.coords.latitude,
        lng: position.lng || position.coords.longitude
      };
      localStorage.setItem('geolocation', JSON.stringify(geolocation)); // setTimeout(() => {getDistanceBetween(geolocation.lat, geolocation.lng)}, 1000)

      (function isGoogleAvailable() {
        if ((typeof google === "undefined" ? "undefined" : _typeof(google)) === "object") {
          onGeopositionChecked(geolocation.lat, geolocation.lng);
        } else {
          setTimeout(isGoogleAvailable, 100);
        }
      })();
    };

    ;

    if (savedGeolocation) {
      onSuccessLocate(savedGeolocation);
    } else {
      $('html').addClass('html_state_hidden');
      $('.b-popup.b-popup_city').addClass('active');
      navigator.geolocation.getCurrentPosition(onSuccessLocate);
    }
  }
}

function onGeopositionChecked(lat, lng) {
  new google.maps.Geocoder().geocode({
    'latLng': new google.maps.LatLng(lat, lng)
  }, function (results, status) {
    if (status == google.maps.GeocoderStatus.OK) {
      for (var component in checkPopUpForm) {
        document.getElementById(component).value = '';
        document.getElementById(component).disabled = false;
      }

      var cityName = results[0].address_components.filter(function (address) {
        return address.types.includes('locality');
      })[0].long_name;
      document.getElementById(component).value = cityName;
      $('.js-checkCity-submit').removeClass('hidden'); // $(document).on('keypress',function(e) {
      //   if(e.which == 13) {
      //     getDistanceBetween(lat, lng);
      //   }
      // });

      if (savedGeolocation) {
        getDistanceBetween(lat, lng);
      }

      $('.js-checkCity-submit').on('click', function () {
        getDistanceBetween(lat, lng);
      });
    } else {
      console.log('wrong geo');
    }
  });
}

function onPlaceChange() {
  var place = autocomplete.getPlace();

  for (var component in checkPopUpForm) {
    document.getElementById(component).value = '';
    document.getElementById(component).disabled = false;
  }

  var cityName = place.name; // var fullAddress = place.formatted_address;

  document.getElementById(component).value = cityName;
  var lat = place.geometry.location.lat();

  var _long = place.geometry.location.lng();

  $('.js-checkCity-submit').addClass('hidden');
  getDistanceBetween(lat, _long);
}

function setCity(city) {
  window.currentCity = city;
}

function setPromoCode(code) {
  window.promocode = code;
}

function getDistanceBetween(lat, _long2) {
  if ($('.b-popup_city.active').length > 0) {
    $('.b-popup_city.active').removeClass('active');
    $('html.html_state_hidden').removeClass('html_state_hidden');
  }

  var origin1 = {
    lat: 55.664775,
    lng: 37.615971
  }; //msk

  var origin2 = {
    lat: 59.927919,
    lng: 30.336618
  }; //spb

  var destinationA = {
    lat: lat,
    lng: _long2
  };
  var service = new google.maps.DistanceMatrixService();
  service.getDistanceMatrix({
    origins: [origin1, origin2],
    destinations: [destinationA],
    travelMode: 'DRIVING',
    unitSystem: google.maps.UnitSystem.METRIC,
    avoidHighways: false,
    avoidTolls: false
  }, function (response, status) {
    if (status !== 'OK') {
      console.log('Error was: ' + status);
    } else {
      var distanceFromOrigin1 = response.rows[0].elements[0].distance && response.rows[0].elements[0].distance.value;
      var distanceFromOrigin2 = response.rows[1].elements[0].distance && response.rows[1].elements[0].distance.value;

      if (distanceFromOrigin1 < 200000) {
        setCity('4400');
      } else if (distanceFromOrigin2 < 200000) {
        setCity('4962');
      } else {
        (0, _api.getNearestCities)(lat, _long2).then(function (data) {
          setCity(data.sort(function (a, b) {
            return a.distance_km - b.distance_km;
          })[0].id);
        });
      }

      (function iscurrentCityAvailable() {
        if (window.currentCity) {
          renderContent(window.currentCity);
        } else {
          setTimeout(iscurrentCityAvailable, 100);
        }
      })();
    }
  });
} // function switchContent(city) { 
//   console.log('city', city);
//   switch (city) {
//     case '4400':
//       renderContent('4400');
//     break;
//     case '4962':
//       renderContent('4962')
//     break;
//     default:
//       renderContent('other')
//     break;    
//   }
// }


function renderContent(cityId) {
  // closePopup();
  // console.log('cityId', cityId);
  var currentContent = content.filter(function (i) {
    return i.cityId === cityId;
  })[0];
  var mask = '+7 (999) 999-99-99';
  if (cityId === 269) mask = '+375 (999) 999-99-99'; // console.log('currentContent', cityId, currentContent);

  (0, _inputmask["default"])(mask, {
    showMaskOnHover: false,
    clearIncomplete: true,
    autoUnmask: true
  }).mask($('.b-form__input[name="customer_phone"]'));
  $('.event').addClass('visible');
  $('.wrapper').removeClass('filter');
  $('html').removeClass('html_state_fixed');

  if (currentContent) {
    $('#js-show-athletes').show();
    $('.event-info-reg__date').html(currentContent.data.date);
    $('.event-info-reg__city').html(currentContent.data.city);
    $('.event-info-reg__place').html(currentContent.data.place);
    $('.event-info-heroes__text').html(currentContent.data.introText);
    $('.signup-form__text').html(currentContent.data.signUpIntroText);
  } else {
    $('.event-info-reg__date').html(otherContent.data.date);
    $('.event-info-reg__city').html(otherContent.data.city);
    $('.event-info-reg__place').html(otherContent.data.place);
    $('.event-info-heroes__text').html(otherContent.data.introText);
    $('.signup-form__text').html(otherContent.data.signUpIntroText);
  } // Dealer centers


  (0, _api.getDealerCenters)(window.currentCity).then(function (data) {
    if (data.length && !currentContent) {
      $('.event-info-reg__city').html(data[0].cityName);
    }

    (0, _libs.insertSelectVal)($(".signup-form .b-form__select[name=\"dealer_center_id\"], .main-form .b-form__select[name=\"dealer_center_id\"]"), data.sort(function (a, b) {
      return a.name.localeCompare(b.name);
    }).map(function (i) {
      return {
        value: i.id,
        title: i.name
      };
    }), true);
  });
}

$('.b-popup__close').on('click', function (e) {
  var $this = $(e.currentTarget);
  e.preventDefault();

  if ($this.closest('.b-popup_city').length) {
    window.location.href = '/';
  } else {
    closePopup();
  }
});

function closePopup() {
  $('html').removeClass('html_state_hidden');
  $('.b-popup').removeClass('active');

  if ($('#current-video').length) {
    $('#current-video').remove();
  }
}

; //Scroll to elem

function scrollTo(elem) {
  $([document.documentElement, document.body]).animate({
    scrollTop: $(elem).offset().top
  }, 1000);
}

$('.js-toggle-heroes').on('click', function (e) {
  var $this = $(e.currentTarget);
  var opened = '_opened';
  var $heroes = $('.event-info-heroes');
  var $heroesPhoto = $('.more__body');
  e.preventDefault();

  if ($this.hasClass(opened)) {
