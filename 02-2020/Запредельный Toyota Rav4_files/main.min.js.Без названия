$('[data-ga]').on('click', function (e) {
  var $this = $(e.currentTarget);
  var type = $this.data('ga');

  switch (type) {
    case 'toyota_center_minsk':
      (0, _libs.trackEvent)('main_banner_by', 'd_center', 'toyota_center_minsk');
      break;

    case 'toyota_center_minsk_vostok':
      (0, _libs.trackEvent)('main_banner_by', 'd_center', 'toyota_center_minsk_vostok');
      break;

    case 'learn_more':
      (0, _libs.trackEvent)('main_banner_by', 'learn_more', window.location.pathname);
      break;

    case 'toyota_center_minsk_footer':
      (0, _libs.trackEvent)('footer_by', 'd_center', 'toyota_center_minsk');
      break;

    case 'toyota_center_minsk_vostok_footer':
      (0, _libs.trackEvent)('footer_by', 'd_center', 'toyota_center_minsk_vostok');
      break;

    default:
      return;
  }
});

var parsedParams = function parsedParams(element, params) {
  return params.map(function (param) {
    if (/^\$/.test(param)) {
      var elem = window;

      if (/^\$this\./.test(param)) {
        elem = element;
      } else if (/^\$value/.test(param)) {
        if (element.value) {
          if (element.options) {
            return _toConsumableArray(element.options).find(function (opt) {
              return opt.value === element.value;
            }).text;
          } else {
            return element.value;
          }
        } else {
          return null;
        }
      }

      return (0, _safeAccess["default"])(elem, param.replace(/^\$[a-zA-Z]*\./, ''));
    }

    return param;
  });
};

var onTrackableEvent = function onTrackableEvent(e) {
  var hrefToGo;
  var $this = $(e.currentTarget);

  if ($this.attr('href') && $this.attr('target') !== '_blank') {
    e.preventDefault();
    hrefToGo = $this.attr('href');
  }

  var params = $this.data('ga-params').split(';');

  if ($this.data('ga-if')) {
    var conds = $this.data('ga-if').split(';');

    for (var i = 0; i < conds.length; i++) {
      if (conds[i] == 'not-empty') {
        if (e.currentTarget.value !== undefined && !e.currentTarget.value) {
          return;
        }
      }
    }
  }

  var parsed = parsedParams(e.currentTarget, params);
  (0, _libs.trackEvent)(parsed[0], parsed[1], parsed[2], function () {
    if (hrefToGo) {
      window.location.href = hrefToGo;
    }
  });
};

['blur', 'change', 'click'].forEach(function (trigger) {
  $(document).on(trigger, "[data-track-trigger=\"".concat(trigger, "\"][data-ga-params]"), onTrackableEvent);
});
$(document).on('click', ":not([data-track-trigger])[data-ga-params]", onTrackableEvent); // /Google analitics
// Header Fixed

fixedHeader();

function fixedHeader() {
  var $header = $('.header');
  var $html = $('html');
  var $window = $(window);
  var $wrapper = $('.wrapper');
  var $document = $(document);
  if ($window.width() < 768) return;
  var that = this;
  var headerHeight = $header.outerHeight();
  var lastScroll = 0;
  var delta = 5;
  var didScroll;
  $(document).on('scroll', function () {
    var curScroll = window.pageYOffset;
    var toggleFixed = curScroll > 0 ? 'addClass' : 'removeClass';
    $header[toggleFixed]('_fixed');
    didScroll = true;

    if ($wrapper.height() <= $window.height()) {
      $header.removeClass('_hidden _fixed');
    }
  });
  setInterval(function () {
    if (didScroll) {
      hasScrolled();
      didScroll = false;
    }
  }, 250);

  function hasScrolled() {
    var st = window.pageYOffset;
    if (Math.abs(lastScroll - st) <= delta) return;

    if (st > lastScroll && st > headerHeight) {
      $header.addClass('_hidden');
    } else {
      if (st + $(window).height() < $(document).height()) {
        $header.removeClass('_hidden');
      }
    }

    lastScroll = st;
  }
} // Video init


var isPlayerReady = false;
var videoState = {
  0: 'complete',
  1: 'play',
  2: 'pause'
};
$('[data-video]').on('click', function (e) {
  var $this = $(e.currentTarget);
  var videoId = $this.attr('href').split('be/')[1];
  var $popup = $('.b-popup_video');
  e.preventDefault();
  $('html').addClass('html_state_hidden');
  $popup.addClass('active').find('.b-popup__body').append($('<div id="current-video">'));
  var name = $this.data('ga-params') ? $this.data('ga-params').split(';')[2] : videoId;
  var category = $this.data('ga-params') ? $this.data('ga-params').split(';')[0] : 'video';
  var isStarted = false;

  var initPlayer = function initPlayer() {
    isPlayerReady = true;
    var player = new YT.Player('current-video', {
      height: '100%',
      width: '100%',
      videoId: videoId,
      controls: 0,
      playerVars: {
        autoplay: 1
      },
      events: {
        onReady: function onReady() {// player.playVideo()
        },
        onStateChange: function onStateChange(e) {
          console.log('e', e);

          if (isStarted && videoState[e.data]) {
            (0, _libs.trackEvent)(category, videoState[e.data], name);
          }

          if (e.data === 1) {
            isStarted = true;
          }
        }
      }
    });
  };

  if (isPlayerReady) {
    initPlayer();
  } else {
    $('body').append($('<script src="https://www.youtube.com/iframe_api">'));
    window.onYouTubeIframeAPIReady = initPlayer;
  } // .append('<iframe id="current-video" width="100%" height="100%" src="https://www.youtube.com/embed/' + videoId + '?autoplay=1&controls=false"  frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>');

});
