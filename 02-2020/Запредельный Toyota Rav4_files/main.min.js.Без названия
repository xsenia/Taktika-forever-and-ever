var formSmsSubmit = function formSmsSubmit() {
  //
  var sms_code = $('input[name="sms_code"]').val();
  var eventData = window.eventFormData;
  var smsId = window.smsData.data.send_sms_code.id;
  var promocode = window.promocode;
  console.log('promocode ', promocode);
  (0, _api.sendReg)(eventData, smsId, sms_code, promocode).then(function (data) {
    (0, _libs.requestLoading)(false);

    if (data.data && data.data.TMRERegisterCustomer) {
      if (data.data.TMRERegisterCustomer.status === "ok") {
        $('.b-popup_sms').find('.b-popup__body').html("\n          <a href=\"/\" class=\"b-popup__close\"></a>\n          <div class=\"b-popup__info\">\n            <p>\u0421\u043F\u0430\u0441\u0438\u0431\u043E, \u0432\u0430\u0448\u0430 \u0437\u0430\u044F\u0432\u043A\u0430 \u043E\u0442\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0430.</p>\n          </div>\n        ");
      }
    }

    if (data.errors) {
      $('.b-popup_sms').find('.b-popup__body').html("\n        <a href=\"/\" class=\"b-popup__close\"></a>\n        <div class=\"b-popup__info\">\n          <p>\u0421\u0435\u0440\u0432\u0438\u0441 \u0432\u0440\u0435\u043C\u0435\u043D\u043D\u043E \u043D\u0435\u0434\u043E\u0441\u0442\u0443\u043F\u0435\u043D, \u043F\u043E\u043F\u0440\u043E\u0431\u0443\u0439\u0442\u0435 \u043E\u0442\u043F\u0440\u0430\u0432\u0438\u0442\u044C \u0444\u043E\u0440\u043C\u0443 \u043F\u043E\u0437\u0434\u043D\u0435\u0435.</p>\n        </div>");

      if (data.errors[0].validation) {
        $('.b-popup_sms').find('.b-popup__body').html("\n          <a href=\"/\" class=\"b-popup__close\"></a>\n          <div class=\"b-popup__info\">\n            <p>\u041E\u0448\u0438\u0431\u043A\u0430 \u0432\u0430\u043B\u0438\u0434\u0430\u0446\u0438\u0438 \u0434\u0430\u043D\u043D\u044B\u0445.</p>\n          </div>");
        Object.values(data.errors[0].validation).map(function (i) {
          for (var j = 0; j < i.length; j++) {
            console.log('44', i[j]);
            $('.b-popup__info').append("<p>".concat(i[j], "</p>"));
          }
        });
      }

      console.log('errors', data.errors[0].message, data.errors[0].validation);
    }
  });
};

function saveData(saveData, name) {
  window["".concat(name)] = saveData;
}

function eventCounter() {
  var end = new Date('11/15/2019 23:59 GMT+3');
  var _second = 1000;

  var _minute = _second * 60;

  var _hour = _minute * 60;

  var _day = _hour * 24;

  var timer;
  showRemaining();

  function declension(num, expressions) {
    var result;
    var count = num % 100;

    if (count >= 5 && count <= 20) {
      result = expressions['2'];
    } else {
      count = count % 10;

      if (count == 1) {
        result = expressions['0'];
      } else if (count >= 2 && count <= 4) {
        result = expressions['1'];
      } else {
        result = expressions['2'];
      }
    }

    return result;
  }

  function showRemaining() {
    var now = new Date();
    var distance = end - now;

    if (distance < 0) {
      clearInterval(timer);
      return;
    }

    var days = Math.floor(distance / _day);
    var hours = Math.floor(distance % _day / _hour);
    var minutes = Math.floor(distance % _hour / _minute);

    if (days < 10) {
      days = '0' + days;
    }

    if (hours < 10) {
      hours = '0' + hours;
    }

    if (minutes < 10) {
      minutes = '0' + minutes;
    }

    document.getElementById('count_days').innerHTML = days;
    document.getElementById('count_days_text').innerHTML = declension(days, ['', '', '']);
    document.getElementById('count_hours').innerHTML = hours;
    document.getElementById('count_hours_text').innerHTML = declension(hours, ['', '', '']);
    document.getElementById('count_minutes').innerHTML = minutes;
    document.getElementById('count_minutes_text').innerHTML = declension(minutes, ['', '', '']);
    $('.counter').addClass('animate_fade');
  }

  timer = setInterval(showRemaining, 1000);
}

