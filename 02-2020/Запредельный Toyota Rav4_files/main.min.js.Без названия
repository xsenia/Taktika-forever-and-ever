  (0, _libs.insertSelectVal)($(".signup-form .b-form__select[name=\"car_brand_id\"]"), data.sort(function (a, b) {
    return a.name.localeCompare(b.name);
  }).map(function (i) {
    return {
      value: i.id,
      title: i.name
    };
  }), {
    value: '0',
    title: ' '
  });
}); // Change brand

$(".signup-form .b-form__select[name=\"car_brand_id\"]").on('change', function (e) {
  var _this = $(this);

  var value = _this.val();

  var _carBrands = carBrands.filter(function (i) {
    return value == i.id;
  })[0];

  if (_carBrands) {
    $(".signup-form .b-form__select[name=\"car_model_id\"]");
    (0, _libs.insertSelectVal)($(".signup-form .b-form__select[name=\"car_model_id\"]"), _carBrands.car_models.sort(function (a, b) {
      return a.name.localeCompare(b.name);
    }).map(function (i) {
      return {
        value: i.id,
        title: i.name
      };
    }), true);
  } else {
    $('.signup-form .b-form__select[name="car_model_id"]').empty().attr('disabled', true).prop('disabled', true).trigger('refresh');
  }
}); // Cities

